#include <stdio.h>
int main(void) {
    int a, b, c;
    scanf("%d %d %d", &a, &b, &c);
    if (b>a && c>b) {
        printf("Up");
    } else {
    
        if (a>b && b>c) {
            printf("down");
        } else {
            printf("negative");
        }    
    
    }
      
        
   
    
    return 0;
}
